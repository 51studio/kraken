{
  "name": "kraken",
  "version": "0.0.0",
  "description": "Web standard based rendering engine.",
  "scripts": {
    "install": "cd cli && npm link --ignore-scripts && rm -rf build && ln -s ../targets build",
    "start": "cd app_launcher && flutter run",
    "build:macos": "node scripts/build_darwin",
    "build:android": "node scripts/build_android",
    "build:linux": "node scripts/build_linux",
    "pretest": "npm i && node scripts/build_darwin_dylib",
    "test": "cd tests && npm test",
    "test:unit": "cd tests && npm run unit",
    "test:integration": "cd tests && npm run integration",
    "lint": "cd kraken && flutter analyze",
    "format": "cd kraken && flutter format",
    "ci": "npm run lint && npm run test:unit && npm run test:bridge",
    "test:bridge": "npm run test:bridge:jsa:v8 && npm run test:bridge:jsa:jsc && npm run test:bridge:kom",
    "test:bridge:jsa:v8": "./targets/darwin/debug/lib/jsa_test_v8",
    "test:bridge:jsa:jsc": "./targets/darwin/debug/lib/jsa_test_jsc",
    "test:bridge:kom": "./targets/darwin/debug/lib/kom_test"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.alibaba-inc.com:kraken/kraken.git"
  },
  "keywords": [
    "kraken",
    "w3c"
  ],
  "devDependencies": {
    "ali-oss": "^6.4.0",
    "chalk": "^3.0.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1"
  },
  "author": "Kraken Team"
}
