{
  "name": "kraken",
  "version": "0.0.0",
  "description": "Web standard based rendering engine.",
  "scripts": {
    "install": "cd cli && npm link --ignore-scripts && rm -rf build && ln -s ../targets build",
    "start": "cd app_launcher && flutter run",
    "build:macos": "node scripts/build_darwin",
    "build:macos:release": "KRAKEN_BUILD=Release node scripts/build_darwin",
    "build:android": "node scripts/build_android_app",
    "build:android:release": "KRAKEN_BUILD=Release node scripts/build_android_app",
    "build:sdk:ios": "node scripts/build_ios_sdk",
    "build:sdk:android": "node scripts/build_android_sdk",
    "build:sdk:android:release": "KRAKEN_BUILD=Release node scripts/build_android_sdk",
    "upload:sdk:android": "KRAKEN_BUILD=Release node scripts/build_android_sdk_and_upload",
    "build:linux": "node scripts/build_linux",
    "upload": "node scripts/macos_pack_and_upload",
    "pretest": "npm i && npm run lint && node scripts/build_darwin_dylib",
    "test": "node scripts/run_test.js",
    "test:unit": "cd tests && npm run unit",
    "test:integration": "cd tests && npm run integration",
    "lint": "cd kraken && flutter analyze",
    "format": "cd kraken && flutter format",
    "ci": "npm i && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.alibaba-inc.com:kraken/kraken.git"
  },
  "keywords": [
    "kraken",
    "w3c"
  ],
  "devDependencies": {
    "ali-oss": "^6.5.1",
    "chalk": "^3.0.0",
    "commander": "^5.0.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "minimist": "^1.2.4",
    "mkdirp": "^0.5.1",
    "rimraf": "^3.0.2"
  },
  "author": "Kraken Team"
}
