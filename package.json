{
  "name": "kraken",
  "version": "0.0.0",
  "description": "Flutter & Web standard based rendering engine for IoT.",
  "scripts": {
    "install": "cd cli && tnpm i --ignore-scripts && tnpm link -g && rm -rf build && ln -s ../targets build",
    "start": "cd app_launcher && flutter run",
    "build:macos": "node scripts/build_darwin",
    "build:android": "node scripts/build_android",
    "build:linux": "node scripts/build_linux",
    "test": "cd test && npm run test",
    "lint": "cd kraken && flutter analyze",
    "format": "cd kraken && flutter format",
    "ci": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.alibaba-inc.com:kraken/kraken.git"
  },
  "keywords": [
    "kraken",
    "rax"
  ],
  "devDependencies": {
    "ali-oss": "^6.4.0",
    "chalk": "^3.0.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1"
  },
  "author": "KrakenTeam"
}
