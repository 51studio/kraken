{
  "private": true,
  "scripts": {
    "pretest": "npm install",
    "clean": "rm -rf ./integration/.specs && rm -rf ./integration/snapshots/*.current.png && rm -rf ./integration/snapshots/*.diff.png",
    "compile:ts": "cd integration && tsc --outDir ./.specs",
    "test": "flutter pub get && flutter clean && npm run integration",
    "prettier": "prettier --no-config --single-quote --trailing-comma=es5 --write ./integration/specs/{.,**,**/**}/*.ts",
    "rax": "mkdir ./integration/.specs/specs/rax  && cp -r ./integration/rax-components/build/* ./integration/.specs/specs/rax",
    "integration": "npm run clean && npm run compile:ts && npm run rax && node scripts/integration_starter.js"
  },
  "devDependencies": {
    "chalk": "^3.0.0",
    "glob": "^7.1.6",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3",
    "ws": "^7.3.0",
    "is-port-reachable": "^3.0.0"
  },
  "dependencies": {
    "rax-text": "^1.3.1"
  }
}